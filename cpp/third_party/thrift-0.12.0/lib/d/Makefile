install-d_asyncDATA: $(d_async_DATA)
	@$(NORMAL_INSTALL)
	@list='$(d_async_DATA)'; test -n "$(d_asyncdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(d_asyncdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(d_asyncdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(d_asyncdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(d_asyncdir)" || exit $$?; \
	done

